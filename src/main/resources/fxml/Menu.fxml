<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.text.Font?>

<BorderPane fx:id="menuPane" styleClass="menu-pane" stylesheets="@../css/design.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <FlowPane alignment="CENTER" columnHalignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" orientation="VERTICAL" prefHeight="400.0" prefWidth="722.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onStartButton" prefHeight="42.0" prefWidth="200.0" styleClass="btn-normal" text="Start">
               <font>
                  <Font name="MedievalSharp" size="22.0" />
               </font>
               <FlowPane.margin>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="350.0" />
               </FlowPane.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#onGridSizeButton" prefHeight="42.0" prefWidth="150.0" styleClass="btn-normal" text="Grid Size">
               <font>
                  <Font name="MedievalSharp" size="22.0" />
               </font>
               <FlowPane.margin>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </FlowPane.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#onPlayerSizeButton" prefHeight="42.0" prefWidth="150.0" styleClass="btn-normal" text="Player Size">
               <font>
                  <Font name="MedievalSharp" size="22.0" />
               </font>
               <FlowPane.margin>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </FlowPane.margin>
            </Button>
            <Button mnemonicParsing="false" onAction="#onQuitButton" prefHeight="42.0" prefWidth="100.0" styleClass="btn-normal" text="Quit">
               <font>
                  <Font name="MedievalSharp" size="22.0" />
               </font>
               <FlowPane.margin>
                  <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
               </FlowPane.margin>
            </Button>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <cursor>
            <Cursor fx:constant="DEFAULT" />
         </cursor>
      </FlowPane>
   </center>
</BorderPane>
